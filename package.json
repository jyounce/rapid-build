{
	"name": "rapid-build",
	"version": "0.71.0",
	"tasksPrefix": "rb-",
	"description": "Rapid build system utilizing Node.js, Express and AngularJS.",
	"homepage": "http://rapid-build.io/",
	"author": {
		"name": "Judd Younce",
		"email": "jyounce@yahoo.com",
		"url": "https://github.com/jyounce"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jyounce/rapid-build"
	},
	"bugs": {
		"url": "https://github.com/jyounce/rapid-build/issues"
	},
	"license": "MIT",
	"engines": {
		"node": ">=4.0.0"
	},
	"preferGlobal": true,
	"main": "./",
	"bin": {
		"rapid-build": "bin/build.js"
	},
	"directories": {
		"lib": "./lib",
		"src": "./src",
		"test": "./test"
	},
	"scripts": {
		"test": "node test",
		"bump": "node scripts/bump",
		"prepublish": "npm run lib-create",
		"postinstall": "npm run lib-unpack-server",
		"lib-create": "npm run server-npm-install && node scripts/lib create-lib",
		"lib-unpack-server": "node scripts/lib unpack-lib-server",
		"server-npm-update": "cd src/src/server/ && npm update && cd ../../../",
		"server-npm-install": "cd src/src/server/ && npm prune && npm install && cd ../../../",
		"server-npm-outdated": "cd src/src/server/ && npm outdated --depth=0 && cd ../../../",
		"outdated": "npm outdated --depth=0 && npm run server-npm-outdated"
	},
	"devDependencies": {
		"conventional-changelog": "1.1.0",
		"prepend-file": "1.3.1",
		"ps-node": "0.1.4",
		"request": "2.79.0"
	},
	"dependencies": {
		"asyncawait": "1.0.6",
		"babel-preset-es2015": "6.22.0",
		"bluebird": "3.4.7",
		"bower": "1.8.0",
		"browser-sync": "2.18.7",
		"browserify": "13.3.0",
		"coffee-script": "1.10.0",
		"colors": "1.1.2",
		"commander": "2.9.0",
		"cson": "4.0.0",
		"deepmerge": "1.3.2",
		"del": "2.2.2",
		"delete-empty": "0.1.3",
		"event-stream": "3.3.4",
		"find-port": "1.0.1",
		"fs-extra": "2.0.0",
		"fstream": "1.0.10",
		"glob-stream": "5.3.5",
		"gulp": "3.9.1",
		"gulp-angular-templatecache": "1.9.1",
		"gulp-babel": "6.1.2",
		"gulp-bless": "3.0.1",
		"gulp-cachebust": "0.0.6",
		"gulp-coffee": "2.3.3",
		"gulp-concat": "2.6.1",
		"gulp-cssnano": "2.1.2",
		"gulp-htmlmin": "3.0.0",
		"gulp-if": "2.0.2",
		"gulp-jsonminify": "1.0.0",
		"gulp-less": "3.3.0",
		"gulp-nodemon": "2.2.1",
		"gulp-plumber": "1.1.0",
		"gulp-rename": "1.2.2",
		"gulp-replace": "0.5.4",
		"gulp-sass": "3.1.0",
		"gulp-sequence": "0.4.6",
		"gulp-template": "4.0.0",
		"gulp-typescript": "2.13.6",
		"gulp-uglify": "2.0.1",
		"gulp-util": "3.0.8",
		"gulp-watch": "4.3.11",
		"jasmine": "2.5.1",
